{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "rootDir": ".",
      "baseUrl": ".",
      "paths": {
        "@fixtures/*": ["fixtures/*"],
        "@security/*":["security/*"],
        "spreadsheet-orm":["../main/src/index.ts"],
        "spreadsheet-orm/*":["../main/src/*"],
        "@src/*": ["../main/src/*"],
        "@built/*": ["../main/dist/*"],
        "@built": ["../main/dist/index.js"]
      },
      "composite": true
    },
    "ts-node": {
      "esm": true
    },
    "include": [
      "index.ts",
      "fixtures/**/*",
      "integration/**/*",
      "unit/**/*",
      "src/**/*",
      "../main/src/**/*"
    ],
    "references": [
      { "path": "../main" }
    ]
  }