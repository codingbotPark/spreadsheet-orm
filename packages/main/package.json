{
  "name": "spreadsheet-orm",
  "version": "0.1.33",
  "description": "ORM for Google Spreadsheet - Query Builder and Schema Management for spreadsheet database",
  "keywords": [
    "google-spreadsheet",
    "spreadsheet-orm",
    "spreadsheet-database",
    "google-sheets-api",
    "orm",
    "database",
    "query-builder"
  ],
  "license": "MIT",

  "packageManager": "yarn@4.5.3",
  "dependencies": {
    "gaxios": "^6.7.1",
    "googleapis": "^144.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf dist",
    "build:js": "yarn node esbuild.config.js",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run build:js && yarn run build:types",
    "start": "tsx src/index.ts",
    "dev": "npm run build:js -- --watch",
    "dev:types": "npm run build:types -- --watch",
    "prepublishOnly": "if [ -z \"$CI\" ]; then echo 'please publish with CI!' && exit 1; fi"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*"
  },
  "files": ["dist"],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/codingbotPark/spreadsheet-orm.git"
  }
}
