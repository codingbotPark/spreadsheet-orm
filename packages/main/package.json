{
  "name": "spreadsheet-orm",
  "version": "1.0.4",
  "description": "ORM for Google Spreadsheet - Query Builder and Schema Management for spreadsheet database",
  "keywords": [
    "google-spreadsheet",
    "spreadsheet-orm",
    "spreadsheet-database",
    "google-sheets-api",
    "orm",
    "database",
    "query-builder"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.5.3",
  "dependencies": {
    "gaxios": "^6.7.1",
    "googleapis": "^144.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "esbuild": "^0.25.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf dist",
    "build:js": "yarn node esbuild.config.js",
    "build:types": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build": "yarn run clean && yarn run build:js && yarn run build:types && yarn run copy:readme",
    "start": "tsx --tsconfig tsconfig.json src/index.ts",
    "dev": "npm run build:js -- --watch",
    "dev:types": "npm run build:types -- --watch",
    "copy:readme": "cp ../../README.md ./",
    "prepublishOnly": "if [ -z \"$CI\" ]; then echo 'please publish with CI!' && exit 1; fi"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "./README.md"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "development": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codingbotPark/spreadsheet-orm.git"
  }
}
